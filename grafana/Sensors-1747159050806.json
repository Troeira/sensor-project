          "rawQuery": true,
          "rawSql": "SELECT \r\n  \"timestamp\", \r\n  CASE \r\n    WHEN \"sensor_id\" = 'sensor1' THEN \"value\"\r\n    ELSE NULL\r\n  END AS \"sensor1\",\r\n  CASE \r\n    WHEN \"sensor_id\" = 'sensor2' THEN \"value\"\r\n    ELSE NULL\r\n  END AS \"sensor2\",\r\n  CASE \r\n    WHEN \"sensor_id\" = 'sensor3' THEN \"value\"\r\n    ELSE NULL\r\n  END AS \"sensor3\"\r\nFROM \"sensor_data\"\r\nWHERE \"sensor_id\" IN ('sensor1', 'sensor2', 'sensor3')\r\nORDER BY \"timestamp\" DESC\r\nLIMIT 300",
